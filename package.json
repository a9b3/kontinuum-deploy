{
  "name": "starter-deck_js-module",
  "version": "0.0.0",
  "description": "Starter-deck for a js module using babel to enable writing in es2015.",
  "main": "lib/index.js",
  "directories": {
    "example": "example"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.7.6",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-webpack": "6.4.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "^2.9.0",
    "eslint-config-esayemm": "^0.1.2",
    "mocha": "^2.5.3",
    "should": "^9.0.0"
  },
  "scripts": {
    "start": "nodemon --watch example --watch src ./example/index.js & babel src --out-dir lib --watch",
    "preversion": "echo \"run tests here\"",
    "version": "npm run prerelease && git add .",
    "postversion": "git push && git push --tags && npm publish",
    "prerelease": "rm -rf lib && babel src --out-dir lib",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel-core/register --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/you/foo.git"
  },
  "author": "Sam Lau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/you/foo/issues"
  },
  "keywords": [],
  "homepage": "https://github.com/you/foo#readme"
}
